pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo "I am in the build stage"
            }
        }
        stage('Test'){
            steps {
                echo "I am in the test stage"
            }
        }
        stage('Deploy') {
            steps {
                echo "I am in the deploy stage"
            }
        }
        stage('Archive') {
            steps {
                echo "I am in the archive stage"
            }
        }
    }
}
